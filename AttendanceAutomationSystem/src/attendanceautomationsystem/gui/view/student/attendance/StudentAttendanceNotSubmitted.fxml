<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="attendanceautomationsystem.gui.controller.student.attendance.StudentAttendanceNotSubmittedController">
   <children>
      <ImageView fx:id="imgAttendance" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/notsubmitted.png" />
         </image>
         <StackPane.margin>
            <Insets bottom="25.0" />
         </StackPane.margin>
      </ImageView>
      <Label text="Attendance for today has not been submitted." textFill="RED">
         <font>
            <Font name="Montserrat Light" size="15.0" />
         </font>
         <StackPane.margin>
            <Insets top="75.0" />
         </StackPane.margin>
      </Label>
      <Label text="Connect to the school network and reload this tab" textFill="RED">
         <font>
            <Font name="Montserrat Light" size="15.0" />
         </font>
         <StackPane.margin>
            <Insets top="125.0" />
         </StackPane.margin>
      </Label>
      <Label text="in order to submit your attendance." textFill="RED">
         <font>
            <Font name="Montserrat Light" size="15.0" />
         </font>
         <StackPane.margin>
            <Insets top="175.0" />
         </StackPane.margin>
      </Label>
   </children>
</StackPane>
